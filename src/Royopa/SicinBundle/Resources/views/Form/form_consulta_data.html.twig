{% extends 'RoyopaSicinBundle:Default:base.html.twig' %}

{% block content %}


<div class="row">
    <div class="col-md-12">

<div class="page-header">
  <h1>{{ title }}</h1>
  {% if subtitle %}
    <p>{{ subtitle }}</p>
  {% endif %}
</div>

<div class="col-xs-3">

{{ form_start(form) }}

    {{ form_errors(form) }}

    {{ form_row(form.date) }}

    {{ form_row(form.submit, { 'label': 'Consultar' }) }}

{{ form_end(form) }}

</div>

</div>
</div>
{% endblock %}
